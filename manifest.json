{
  "manifest_version": 3,
  "name": "Website Tracker",
  "version": "1.0",
  "permissions": ["tabs", "storage", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "hello_extensions.png"
  },
  "icons": {
    "128": "hello_extensions.png"
  }
}
